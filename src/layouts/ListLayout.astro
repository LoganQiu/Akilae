---
import BaseLayout from './BaseLayout.astro';

export interface Props {
  title?: string;
  subtitle?: string;
  description?: string;
  keywords?: string[];
}

const { title, subtitle, description, keywords } = Astro.props;
---

<BaseLayout 
  title={title} 
  subtitle={subtitle} 
  description={description} 
  keywords={keywords}
  pageType="list"
>
  <slot />
</BaseLayout>